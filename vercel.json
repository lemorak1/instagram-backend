{
    "version": 2,
    "builds": [
      {
        "src": "src/index.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/privacy-policy.html",
        "dest": "/public/privacy-policy.html"
      },
      {
        "src": "/(.*)",
        "dest": "src/index.ts"
      }
    ]
  }
  